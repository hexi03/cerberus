@model cerberus.Models.Reports.WHReplenishmentReport


<div class="container">
    <h2>Отчет о приемке РМ на склад</h2>
    <hr />

    <p>Склад: @ViewBag.WareHouses[Model.warehouse_id].name</p>

    <h5>Приянто:</h5>
    <div>
        @if (Model.items.Count() == 0)
        {
            <div>
                <p>Ничего</p>
            </div>
        }
        @foreach (var item in Model.items)
        {
            <div>
                <p>@ViewBag.Items[item.Key].name : @item.Value</p>
            </div>
        }


    </div>
</div>
