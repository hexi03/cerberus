@model cerberus.Models.ViewModels.Reports.WHReleaseReportFormViewModel

<div class="container">
    <h2>Отчет о выдаче РМ</h2>
    <hr />

    <p>Склад: @ViewBag.WareHouses[Model.warehouse_id].name</p>

    <p>Отчет о запросе РМ: @ViewBag.Reports[@Model.supply_requirement_id].timestamp</p>


    <h5>Список предоставленного:</h5>
    <div>
        @if (Model.items.Count() == 0)
        {
            <div>
                <p>Ничего</p>
            </div>
        }
        @foreach (var item in Model.items)
        {
            <div>
                <p>@ViewBag.Items[item.Key].name : @item.Value</p>
            </div>
        }


    </div>

</div>

