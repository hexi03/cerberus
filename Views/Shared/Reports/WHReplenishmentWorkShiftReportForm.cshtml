@model cerberus.Models.Reports.WHWorkShiftReplenishmentReport

@using (Html.BeginForm("Reports", "WHWorkShiftReplenishmentReport", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { role = "form" }))
{
    @Html.AntiForgeryToken()

<div class="form-horizontal">
    <h4>Report</h4>
    <hr />
    <input name="report_type" type="hidden" value="@cerberus.Models.Report.Types.WHWorkShiftReplenishment.ToString()">
    <input name="warehouse_id" type="hidden" value="@ViewBag.warehouse_id">

    <div class="keyValuePairs item-list-controlable-container" data-dname="items">

        <button type="button" class="addKeyValuePair">+</button>
    </div>


    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Create" class="btn btn-default" />
        </div>
    </div>
</div>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>
